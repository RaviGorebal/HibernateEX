<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package = "com.cluster.hibernate.association.unidirectional.many2one">
	<class name="Student" table = "STUDENT_M21">
		<id name="sid" column="STUDENT_ID" type = "int">
			<generator class="increment"/>
		</id>
		<property name="studentName" column = "STUDENT_NAME"/>	
		<many-to-one name="address" 
			column="ADDRESS_ID"
			not-null = "true"
			class = "Address"/>
	</class>
		
	<class name="Address" table = "ADDRESS_M21">
		<id name="addressId" column="ADDRESS_ID" type = "int">
			<generator class="increment" />
		</id>
		<property name="zipCode" column = "ZIPCODE"/>
	</class>
</hibernate-mapping>
